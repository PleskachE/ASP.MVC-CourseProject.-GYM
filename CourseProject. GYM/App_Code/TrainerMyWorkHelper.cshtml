@using CourseProject.GYM.Models

@helper HtmlList(List<ScheduleModel> model)
{
    foreach (var item in model)
    {
        <tr>
            <td>@item.TrainingName</td>
            <td>@item.NameHall</td>
            <td>@item.StartTime</td>
            <td>@item.EndTime</td>
            <td><a href="/Тrainer/Users/@item.Id">Посмотреть участников</a></td>
        </tr>
    }
}
